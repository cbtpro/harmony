{
  "name": "harmony",
  "version": "1.0.0",
  "description": "基于electron、vite、vue3的文件管理系统，让混沌的文件变得和谐。",
  "main": "dist-electron/main/index.js",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "dev:staging": "vite --mode staging",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:staging": "vue-tsc --noEmit && vite --mode staging build && electron-builder",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cbtpro/harmony.git"
  },
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "文件管理",
    "跨平台"
  ],
  "author": "陈碧滔 cbtpro@163.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cbtpro/harmony/issues"
  },
  "homepage": "https://github.com/cbtpro/harmony#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.1",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.12.1",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "element-plus": "^2.2.32",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  }
}
